{
  "version": 2,
  "functions": {
    "server/index.php": { "runtime": "vercel-php@0.7.4" }
  },
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/storage/(.*)", "dest": "/public/storage/$1" },
    { "src": "/(.*)", "dest": "/server/index.php" }
  ],
  "outputDirectory": "public"
}