{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compress-assets": "node scripts/compress-assets.js",
    "download-fonts": "node scripts/download-threejs-fonts.js",
    "optimize-fonts": "node scripts/optimize-fonts.js",
