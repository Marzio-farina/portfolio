    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.2.3",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "sharp": "^0.33.1",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.2"
